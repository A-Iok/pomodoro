{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "noUnusedParameters": false,
    "strict": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "vite.config.ts",
    "src/**/*"
  ]
}